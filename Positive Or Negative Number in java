import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args)
	{
		//your code here
		int n;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		if(n>=0){
			System.out.println("Number is positive");
	}
		else
			{
			System.out.println("Number is negative");
	}
	}
}
