import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) 
	{
		    int r,sum=0,temp;    
  int n;
		Scanner sc=new Scanner (System.in);
		n=sc.nextInt();//It is the number variable to be checked for palindrome  
  
  temp=n;    
  while(n>0){    
   r=n%10;  //getting remainder  
   sum=(sum*10)+r;    
   n=n/10;    
  }    
  if(temp==sum)    
   System.out.println("true ");    
  else    
   System.out.println("false");     

	}
}
