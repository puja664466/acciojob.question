import java.util.*;
  public class Main {
  public static void main(String[] args) {
  int tot_theory_marks , tot_practical_marks , tot_marks;
Scanner sc=new Scanner(System.in);
	  tot_theory_marks=sc.nextInt();
	  tot_practical_marks=sc.nextInt();
	  tot_marks=sc.nextInt();
  int percentage_of_marks = ((tot_theory_marks + tot_practical_marks)*100)/tot_marks;  
    System.out.print(""+percentage_of_marks+"%\n");
  }
}
